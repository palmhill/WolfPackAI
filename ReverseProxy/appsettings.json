{
  "AzureAd": {
    "TenantId": "your-tenant-id",
    "ClientId": "your-client-id",
    "Instance": "https://login.microsoftonline.com/"
  },
  "ReverseProxy": {
    "Routes": {
      "openwebui-route": {
        "ClusterId": "openwebui-cluster",
        "Match": {
          "Path": "{**catch-all}"
        },
        "AuthorizationPolicy": "default"
      },
      "litellm-route": {
        "ClusterId": "litellm-cluster",
        "Match": {
          "Path": "/api/litellm/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/{**catch-all}"
          }
        ],
        "AuthorizationPolicy": "default"
      }
    },
    "Clusters": {
      "openwebui-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://openwebui"
          }
        }
      },
      "litellm-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://litellm"
          }
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
