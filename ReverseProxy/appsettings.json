{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:80"
      },
      "Https": {
        "Url": "https://0.0.0.0:443"
      }
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "litellm-route": {
        "ClusterId": "litellm-cluster",
        "Order": 1,
        "Match": {
          "Path": "/litellm/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/litellm"
          },
          {
            "RequestHeader": "X-Forwarded-For",
            "Append": "{RemoteIpAddress}"
          },
          {
            "RequestHeader": "X-Forwarded-Proto",
            "Set": "{Scheme}"
          },
          {
            "RequestHeader": "X-Forwarded-Host",
            "Set": "{Host}"
          },
          {
            "RequestHeader": "X-Forwarded-Prefix",
            "Set": "/litellm"
          }
        ]
      },
      "n8n-route": {
        "ClusterId": "n8n-cluster",
        "Order": 2,
        "Match": {
          "Path": "/n8n/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/n8n"
          },
          {
            "RequestHeader": "X-Forwarded-For",
            "Append": "{RemoteIpAddress}"
          },
          {
            "RequestHeader": "X-Forwarded-Proto",
            "Set": "{Scheme}"
          },
          {
            "RequestHeader": "X-Forwarded-Host",
            "Set": "{Host}"
          },
          {
            "RequestHeader": "X-Forwarded-Prefix",
            "Set": "/n8n"
          }
        ]
      },
      "n8n-rest-route": {
        "ClusterId": "n8n-cluster",
        "Order": 3,
        "Match": {
          "Path": "/rest/{**catch-all}"
        },
        "Transforms": [
          {
            "RequestHeader": "X-Forwarded-For",
            "Append": "{RemoteIpAddress}"
          },
          {
            "RequestHeader": "X-Forwarded-Proto",
            "Set": "{Scheme}"
          },
          {
            "RequestHeader": "X-Forwarded-Host",
            "Set": "{Host}"
          },
          {
            "RequestHeader": "X-Forwarded-Prefix",
            "Set": "/n8n"
          }
        ]
      },
      "n8n-assets-route": {
        "ClusterId": "n8n-cluster",
        "Order": 60,
        "Match": {
          "Path": "/n8n/assets/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/n8n"
          },
          {
            "RequestHeader": "X-Forwarded-For",
            "Append": "{RemoteIpAddress}"
          },
          {
            "RequestHeader": "X-Forwarded-Proto",
            "Set": "{Scheme}"
          },
          {
            "RequestHeader": "X-Forwarded-Host",
            "Set": "{Host}"
          },
          {
            "RequestHeader": "X-Forwarded-Prefix",
            "Set": "/n8n"
          }
        ]
      },
      "openwebui-static-route": {
        "ClusterId": "openwebui-cluster",
        "Order": 50,
        "Match": {
          "Path": "/_app/{**catch-all}"
        },
        "Transforms": [
          {
            "RequestHeader": "X-Forwarded-For",
            "Append": "{RemoteIpAddress}"
          },
          {
            "RequestHeader": "X-Forwarded-Proto",
            "Set": "{Scheme}"
          },
          {
            "RequestHeader": "X-Forwarded-Host",
            "Set": "{Host}"
          },
          {
            "RequestHeader": "X-Forwarded-Prefix",
            "Set": "/chat"
          }
        ]
      },
      "openwebui-static2-route": {
        "ClusterId": "openwebui-cluster",
        "Order": 51,
        "Match": {
          "Path": "/static/{**catch-all}"
        },
        "Transforms": [
          {
            "RequestHeader": "X-Forwarded-For",
            "Append": "{RemoteIpAddress}"
          },
          {
            "RequestHeader": "X-Forwarded-Proto",
            "Set": "{Scheme}"
          },
          {
            "RequestHeader": "X-Forwarded-Host",
            "Set": "{Host}"
          },
          {
            "RequestHeader": "X-Forwarded-Prefix",
            "Set": "/chat"
          }
        ]
      },
      "openwebui-assets-route": {
        "ClusterId": "openwebui-cluster",
        "Order": 52,
        "Match": {
          "Path": "/assets/{**catch-all}"
        },
        "Transforms": [
          {
            "RequestHeader": "X-Forwarded-For",
            "Append": "{RemoteIpAddress}"
          },
          {
            "RequestHeader": "X-Forwarded-Proto",
            "Set": "{Scheme}"
          },
          {
            "RequestHeader": "X-Forwarded-Host",
            "Set": "{Host}"
          },
          {
            "RequestHeader": "X-Forwarded-Prefix",
            "Set": "/chat"
          }
        ]
      },
      "openwebui-api-route": {
        "ClusterId": "openwebui-cluster",
        "Order": 53,
        "Match": {
          "Path": "/api/{**catch-all}"
        },
        "Transforms": [
          {
            "RequestHeader": "X-Forwarded-For",
            "Append": "{RemoteIpAddress}"
          },
          {
            "RequestHeader": "X-Forwarded-Proto",
            "Set": "{Scheme}"
          },
          {
            "RequestHeader": "X-Forwarded-Host",
            "Set": "{Host}"
          },
          {
            "RequestHeader": "X-Forwarded-Prefix",
            "Set": "/chat"
          }
        ]
      },
      "openwebui-route": {
        "ClusterId": "openwebui-cluster",
        "Order": 100,
        "Match": {
          "Path": "/chat/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/chat"
          },
          {
            "RequestHeader": "X-Forwarded-For",
            "Append": "{RemoteIpAddress}"
          },
          {
            "RequestHeader": "X-Forwarded-Proto",
            "Set": "{Scheme}"
          },
          {
            "RequestHeader": "X-Forwarded-Host",
            "Set": "{Host}"
          },
          {
            "RequestHeader": "X-Forwarded-Prefix",
            "Set": "/chat"
          }

        ]
      }
    },
    "Clusters": {
      "litellm-cluster": {
        "Destinations": {
          "litellm-destination": {
            "Address": "http://localhost:4000/litellm"
          }
        }
      },
      "openwebui-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:8080/"
          }
        }
      },
      "n8n-cluster": {
        "Destinations": {
          "n8n-destination": {
            "Address": "http://localhost:5678/"
          }
        }
      }
    }
  }
}